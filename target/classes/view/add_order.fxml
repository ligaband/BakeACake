<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="700.0"
            prefWidth="700.0" style="-fx-background-color:#899494" xmlns="http://javafx.com/javafx/17"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.bakeacake.bakeacaketest.controller.AddOrderController">
    <children>
        <VBox alignment="CENTER" layoutX="50.0" layoutY="135.0" maxHeight="-Infinity" maxWidth="-Infinity"
              minHeight="-Infinity" minWidth="-Infinity" prefHeight="456.0" prefWidth="600.0" spacing="10.0"
              xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
            <children>
                <Text fill="#186565" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-font-size: 18px"
                      text="Add Order"/>
                <ChoiceBox fx:id="clientListField" prefWidth="175.0"/>
                <ChoiceBox fx:id="cakeTitleField" prefWidth="175.0"/>
                <ComboBox fx:id="tinSizeField" prefWidth="175.0"/>
                <DatePicker fx:id="datePicker"/>
                <TextField fx:id="timeField" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="175.0"
                           promptText="Pick-up or delivery time (00:00)"/>
                <ChoiceBox fx:id="deliveryField" prefWidth="175.0"/>
                <TextArea fx:id="descriptionField" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
                          minWidth="-Infinity" prefHeight="70.0" prefWidth="175.0" promptText="Description"/>
                <ChoiceBox fx:id="statusField" layoutX="223.0" layoutY="211.0" prefWidth="175.0"/>
                <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="40.0">
                    <children>
                        <Button mnemonicParsing="false" onAction="#handleAddOrder" text="Add Order"/>
                        <Button mnemonicParsing="false" onAction="#handleReturn" text="Return"/>
                    </children>
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                    </padding>
                </HBox>
                <Label fx:id="labelNote"/>
            </children>
        </VBox>
        <Button fx:id="homeButton" layoutX="50.0" layoutY="53.0" mnemonicParsing="false" onAction="#returnHome"
                prefHeight="39.0" prefWidth="41.0"/>
        <Button layoutX="583.0" layoutY="51.0" mnemonicParsing="false" onAction="#handleLogout" text="Log out"/>
    </children>
</AnchorPane>
